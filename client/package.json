  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@hawkeyesec/scanner-cli": "^1.8.0",
    "@types/mockdate": "^2.0.0",
    "@types/react-dates": "^21.8.0",
    "json-server": "^0.16.2",
    "lint-staged": "^10.4.2",
    "mockdate": "^3.0.2",
    "prettier": "^2.1.2"
  },
  "lint-staged": {
    "*.{js,jsx,md,ts,tsx}": [
      "npm run lint",
      "prettier --write --ignore-unknown --config ../.prettierrc.json"
    ]
  }
}
