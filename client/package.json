{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "^3.4.1",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.51",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "apexcharts": "^3.20.0",
    "axios": "^0.20.0",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-apexcharts": "^1.3.7",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "react-test-renderer": "^16.13.1",
    "typescript": "^3.7.5"
  },
  "proxy": "http://localhost:4000",
  "scripts": {
    "precommit": "lint-staged",
    "start": "CI=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "test:precommit": "npm test -- --bail",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix ./",
    "start-stub-server": "json-server stub-server/co2estimations.json --port 4000 --routes stub-server/routes.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "json-server": "^0.16.1",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1"
  },
  "lint-staged": {
    "*.{js,jsx,md,ts,tsx}": [
      "npm run lint",
      "prettier --write --ignore-unknown --config ../.prettierrc.json",
      "npm run test:precommit"
    ]
  }
}
