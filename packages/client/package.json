{
  "name": "@cloud-carbon-footprint/client",
  "version": "0.3.0",
  "dependencies": {
    "@babel/runtime": "^7.12.18",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@types/lodash": "^4.14.168",
    "apexcharts": "^3.25.0",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-apexcharts": "^1.3.7",
    "react-dates": "^21.8.0",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "react-test-renderer": "^17.0.1",
    "styled-components": "^5.2.1",
    "typescript": "4.2.3"
  },
  "proxy": "http://localhost:4000",
  "scripts": {
    "precommit": "lint-staged --no-stash && yarn test --coverage --bail",
    "start": "react-scripts start",
    "start-with-mock-data": "concurrently \"yarn start\" \"yarn start-stub-server\"",
    "sonar:scan": "sonar-scanner",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build",
    "test": "react-scripts --expose-gc test --watchAll=false",
    "test:snapshot": "yarn test -u",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src/",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix ./src/",
    "start-stub-server": "json-server stub-server/co2estimations.json --port 4000 --routes stub-server/routes.json",
    "start:webprod": "node index.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^7.29.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^13.0.7",
    "@types/jest": "^26.0.20",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^14.14.31",
    "@types/ramda": "^0.27.38",
    "@types/react": "^17.0.2",
    "@types/react-dates": "^21.8.1",
    "@types/react-dom": "^17.0.1",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "concurrently": "^6.0.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-unused-imports": "^1.1.0",
    "immer": "^9.0.1",
    "jest": "26.6.3",
    "json-server": "^0.16.3",
    "lint-staged": "^10.5.4",
    "mockdate": "^3.0.2",
    "prettier": "^2.2.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "prettier --write --ignore-unknown --config ../../.prettierrc.json"
    ],
    "*.{md}": [
      "prettier --write --ignore-unknown --config ../../.prettierrc.json"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 91,
        "lines": 94,
        "statements": 93
      }
    }
  }
}
